{
  "name": "sw2dts",
  "version": "2.1.4",
  "description": "Generates TypeScript definition file(d.ts) from swagger.json",
  "keywords": [
    "typescript",
    "swagger",
    "json",
    "yaml",
    "d.ts"
  ],
  "main": "index.js",
  "bin": {
    "sw2dts": "./bin/sw2dts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mstssk/sw2dts.git"
  },
  "scripts": {
    "clean": "rimraf '+(lib|test)/*.js?(.map)' 'test/*.d.ts'",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "version": "npm run changelog && git add CHANGELOG.md",
    "prebuild": "tslint './lib/**/*.ts' './test/**/*.ts'",
    "build": "tsconfig -u && tsfmt -r && tsc -p ./",
    "pretest": "npm run clean && npm run build",
    "test": "mocha test/*.js"
  },
  "author": "mstssk",
  "license": "MIT",
  "dependencies": {
    "commandpost": "^1.0.0",
    "dtsgenerator": "0.7.1",
    "js-yaml": "^3.6.1"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^1.2.0",
    "dtsm": "^1.1.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "nexpect": "^0.5.0",
    "rimraf": "^2.5.2",
    "tsconfig-cli": "^2.0.0",
    "tslint": "^3.9.0",
    "typescript": "^1.8.10",
    "typescript-formatter": "^2.1.0"
  }
}
