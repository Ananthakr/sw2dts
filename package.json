{
  "name": "sw2dts",
  "version": "2.6.1",
  "description": "Generates TypeScript definition file(d.ts) from swagger.json",
  "keywords": [
    "typescript",
    "swagger",
    "json",
    "yaml",
    "d.ts"
  ],
  "main": "index.js",
  "bin": {
    "sw2dts": "./bin/sw2dts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mstssk/sw2dts.git"
  },
  "scripts": {
    "clean": "rimraf '+(lib|test)/*.js?(.map)' '+(lib|test)/*.d.ts'",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "npm run changelog && git add CHANGELOG.md",
    "prebuild": "tslint './lib/**/*.ts' './test/**/*.ts'",
    "build": "tsfmt -r && tsc -p ./",
    "pretest": "npm run clean && npm run build",
    "test": "mocha test/*.js"
  },
  "author": "mstssk",
  "license": "MIT",
  "dependencies": {
    "commandpost": "^1.4.0",
    "dtsgenerator": "^1.2.0",
    "js-yaml": "^3.13.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.12.0",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.6",
    "@types/nexpect": "^0.4.30",
    "@types/node": "^11.11.5",
    "@types/rimraf": "^2.0.2",
    "conventional-changelog-cli": "^2.0.12",
    "mkdirp": "^0.5.1",
    "mocha": "^6.0.2",
    "nexpect": "^0.5.0",
    "rimraf": "^2.6.3",
    "typescript": "^3.3.4000",
    "typescript-formatter": "^7.2.2"
  }
}
