{
  "name": "sw2dts",
  "version": "0.1.0",
  "description": "Generates TypeScript definition file(d.ts) from swagger.json",
  "main": "index.js",
  "bin": {
    "sw2dts": "./bin/sw2dts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mstssk/sw2dts.git"
  },
  "scripts": {
    "postinstall": "dtsm install",
    "build": "tsconfig -u && tsfmt -r && tsc -p ./",
    "tslint": "tslint ./src/**/*.ts ./test/**/*.ts",
    "pretest": "npm run build",
    "test": "mocha test/*.js"
  },
  "author": "mstssk",
  "license": "MIT",
  "dependencies": {
    "commandpost": "^1.0.0",
    "dtsgenerator": "0.6.1"
  },
  "devDependencies": {
    "dtsm": "^1.1.0",
    "mocha": "^2.4.5",
    "nexpect": "^0.5.0",
    "tsconfig-cli": "^2.0.0",
    "tslint": "^3.9.0",
    "typescript": "^1.8.10",
    "typescript-formatter": "^2.1.0"
  }
}
