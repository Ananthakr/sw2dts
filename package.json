{
  "name": "sw2dts",
  "version": "2.5.0",
  "description": "Generates TypeScript definition file(d.ts) from swagger.json",
  "keywords": [
    "typescript",
    "swagger",
    "json",
    "yaml",
    "d.ts"
  ],
  "main": "index.js",
  "bin": {
    "sw2dts": "./bin/sw2dts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mstssk/sw2dts.git"
  },
  "scripts": {
    "clean": "rimraf '+(lib|test)/*.js?(.map)' '+(lib|test)/*.d.ts'",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "npm run changelog && git add CHANGELOG.md",
    "prebuild": "tslint './lib/**/*.ts' './test/**/*.ts'",
    "build": "tsfmt -r && tsc -p ./",
    "pretest": "npm run clean && npm run build",
    "test": "mocha test/*.js"
  },
  "author": "mstssk",
  "license": "MIT",
  "dependencies": {
    "commandpost": "^1.3.0",
    "dtsgenerator": "^0.9.9",
    "js-yaml": "^3.11.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.11.1",
    "@types/mkdirp": "^0.5.1",
    "@types/mocha": "^2.2.44",
    "@types/nexpect": "^0.4.30",
    "@types/node": "^8.0.52",
    "@types/rimraf": "^2.0.2",
    "conventional-changelog-cli": "^1.3.5",
    "mkdirp": "^0.5.1",
    "mocha": "^4.0.1",
    "nexpect": "^0.5.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "typescript-formatter": "^7.0.0"
  }
}
